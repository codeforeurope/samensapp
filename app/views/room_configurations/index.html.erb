<h1>Listing room_configurations</h1>

<table>
  <tr>
    <th>Room</th>
    <th>Name</th>
    <th>Capacity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @room_configurations.each do |room_configuration| %>
  <tr>
    <td><%= room_configuration.room_id %></td>
    <td><%= room_configuration.name %></td>
    <td><%= room_configuration.capacity %></td>
    <td><%= link_to 'Show', room_configuration %></td>
    <td><%= link_to 'Edit', edit_room_configuration_path(room_configuration) %></td>
    <td><%= link_to 'Destroy', room_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Room configuration', new_room_configuration_path %>
